# Environment variables for act
# These simulate GitHub Actions environment

# Repository information
GITHUB_REPOSITORY=provide-io/pyvider-hcl
GITHUB_REPOSITORY_OWNER=provide-io
GITHUB_ACTOR=tim

# Git references
GITHUB_SHA=local-test
GITHUB_REF=refs/heads/main
GITHUB_REF_NAME=main
GITHUB_HEAD_REF=main
GITHUB_BASE_REF=main

# Workflow information
GITHUB_WORKFLOW=Local Test
GITHUB_RUN_ID=local-1
GITHUB_RUN_NUMBER=1
GITHUB_RUN_ATTEMPT=1

# GitHub URLs
GITHUB_SERVER_URL=https://github.com
GITHUB_API_URL=https://api.github.com
GITHUB_GRAPHQL_URL=https://api.github.com/graphql

# Runner information
RUNNER_OS=Linux
RUNNER_ARCH=X64
RUNNER_TEMP=/tmp
RUNNER_TOOL_CACHE=/opt/hostedtoolcache

# CI environment
CI=true
GITHUB_ACTIONS=true

# For act detection in workflows
ACT=true